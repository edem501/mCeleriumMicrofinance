
@{
    ViewBag.Title = "TransferClient";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
<script type="text/javascript" language="javascript">
    function ClearResults() {
        $("#AgentPartial").empty;
    }
</script>
<div class="row">
    <h3><i class="fa fa-users"></i>Transfer de membre</h3>
    <hr />
@using (Ajax.BeginForm("_MemberPartial", "Admin", new AjaxOptions
{
    HttpMethod = "GET",
    UpdateTargetId = "AccountPartial",
    InsertionMode = InsertionMode.Replace,
    LoadingElementId = "Spinner",
    OnBegin = "ClearResults"

}))
        {
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">

                <div class="input-group ">
                    <span class="input-group-addon"><i class="fa fa-user bigger-110"></i></span>
                    @Html.DropDownList("Comm", null, "Transaferer de", new { @class = "form-control dropdown" })
                    <input type="submit" value="Go" class="btn btn-sm" />
                </div>
            </div>
        </div>
        <div class="col-md-4">

        </div>
    </div>
        }
<div id="Spinner" style="display:none;">
    <img src="~/Images/spiffygif.gif" />
</div>

<div id="AccountPartial" class="row">

</div>
    
</div>

